{
  "name": "rb-sink",
  "config": {
    "connector.class": "io.confluent.connect.elasticsearch.ElasticsearchSinkConnector",
    "topics": "corporate-entries, announcements",
    "input.data.format": "PROTOBUF",
    "connection.url": "http://elasticsearch:9200",
    "type.name": "rb-entries",
    "key.ignore": "false",
    "schema.ignore": "true",
    "tasks.max": "2"
  }
}
